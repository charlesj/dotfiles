"*":
  "activate-power-mode":
    autoToggle: false
    comboMode:
      activationThreshold: 100
      exclamationEvery: 50
      opacity: 0.3
      streakTimeout: 5
    excludedFileTypes: {}
    particles:
      colours:
        type: "random"
      size:
        min: 1
    playAudio: {}
    screenShake:
      enabled: false
  core:
    allowPendingPaneItems: false
    projectHome: "/home/josh/workspace"
    telemetryConsent: "no"
    themes: [
      "seti-ui"
      "seti-syntax"
    ]
  editor:
    atomicSoftTabs: false
    autoIndentOnPaste: false
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    tabLength: 4
  "exception-reporting":
    userId: "c476b79b-7ca6-25d7-2ead-66bf6ce99e62"
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
  "seti-ui":
    compactView: true
    disableAnimations: true
  welcome:
    showOnStartup: false
  zentabs:
    neverCloseUnsaved: true
